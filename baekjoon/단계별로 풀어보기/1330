import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        String answer = "";
        
        if (a > b) {
            answer = ">";
        } else if (a < b) {
            answer = "<";
        } else if (a == b) {
            answer = "==";
        }
        
        System.out.print(answer);
    }
}
